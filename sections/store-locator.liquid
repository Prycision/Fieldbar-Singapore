<style>

.fieldbar-retailers-map {
  display: flex;
  height: 100vh;
  width: 100vw;
  font-family: 'Gotham Narrow Book', sans-serif;
}
.fieldbar-retailers-map__map {
  flex: 2;
  height: 100%;
  z-index: 0;
}
.fieldbar-retailers-map__panel {
  flex: 1;
  background: #F3EFEB;
  display: flex;
  flex-direction: column;
  overflow: hidden;
  position: relative;
  padding: 0 1em;
}
.fieldbar-retailers-map__header {
  position: sticky;
  top: 0;
  height: 12em;
  background-color: #F3EFEB;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  z-index: 1;
  padding-bottom: 1em;
}
.fieldbar-retailers-map__header h2 {
  font-size: 1.5em;
  color: #002B4D;
  margin: 0;
  text-align: center;
}
.fieldbar-retailers-map__search {
  position: relative;
  margin-top: 0.5em;
}
.fieldbar-retailers-map__search input {
  width: 100%;
  padding: 0.75em 2.5em 0.75em 0.75em;
  background: #F3EFEB;
  border: 1px solid rgba(0, 0, 0, 0.1);
  border-radius: 6px;
  font-size: 0.85em;
  color: #002B4D;
}
.fieldbar-retailers-map__search input::placeholder {
  color: rgba(0, 0, 0, 0.4);
}
.fieldbar-retailers-map__search::after {
  content: '';
  position: absolute;
  right: 0.75em;
  top: 50%;
  transform: translateY(-50%);
  width: 18px;
  height: 18px;
  background-image: url('https://cdn.shopify.com/s/files/1/0729/3753/7840/files/serc_icon_18153d24-c5f5-4871-ab1e-cb1a8a9b34fa.svg?v=1750153912');
  background-repeat: no-repeat;
  background-size: contain;
  background-position: center;
  pointer-events: none;
  opacity: 0.5;
}
.fieldbar-retailers-map__list {
  flex: 1;
  overflow-y: auto;
  scroll-behavior: smooth;
}
.flagship-icon {
  width: 16px;
  height: 16px;
  vertical-align: middle;
  margin-right: 0.1em;
  display: inline-block;
}
.leaflet-popup-content {
  margin: 0 !important;
  padding: 1em 1.5em !important;
  color: #002B4D;
  font-family: 'Gotham Narrow Book', sans-serif;
  font-size: 1.5em;
  line-height: 1.4em;
}

.leaflet-popup-content strong {
  display: block;
  font-family: 'Gotham Medium', sans-serif;
  text-transform: uppercase;
  color: #002B4D;
  font-size: 1.4em; /* Adjusted size */
  margin-bottom: 0.4em;
}

.leaflet-popup-content .popup-button {
  display: inline-block;
  margin-top: 1em;
  background: #002B4D;
  color: white;
  padding: 0.5em 1em;
  font-size: 1em;
  text-decoration: none;
  text-align: center;
  border-radius: 2px;
  transition: background 0.3s ease;
  text-transform: uppercase; /* Button text in uppercase */
  letter-spacing: 0.05em;
}

.leaflet-popup-content .popup-button:hover {
  background: #FF8A3D;
}

.leaflet-container a.leaflet-popup-close-button {
  color: #002B4D;
  font-size: 1.5em;
  padding: 0.5em 0.75em;
  line-height: 1em;
}

.leaflet-control-attribution {
  display: none !important;
}
.retailer-card {
  padding: 1em;
  margin-bottom: 1em;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  scroll-margin-top: 8em;
}
.retailer-card h3 {
  margin: 0;
  font-family: 'Gotham Medium', sans-serif !important;
  text-transform: uppercase;
  color: #002B4D;
  transition: color 0.3s ease;
  cursor: pointer;
}
.retailer-card:hover h3,
.retailer-card.active h3 {
  color: #FF8A3D;
}
.retailer-card details {
  margin-top: 0.5em;
}
.retailer-card summary {
  list-style: none;
  position: relative;
  padding-right: 1.5em;
  font-weight: bold;
  cursor: pointer;
  font-family: 'Gotham Narrow Book', sans-serif;
}
.retailer-card summary::after {
  content: '+';
  position: absolute;
  right: 0;
  top: 0;
  font-size: 1.2em;
  transition: transform 0.3s ease;
}
.retailer-card details[open] summary::after {
  content: '−';
}
.retailer-card details div {
  margin-top: 0.5em;
  padding-left: 0.5em;
}
.retailer-card .online-only-note {
  color: #FF8A3D;
  font-style: italic;
  font-size: 0.9em;
  display: block;
  margin-top: 0.25em;
}
.view-all-button {
  padding: 1em;
  text-align: center;
  background: #002B4D;
  color: white;
  font-size: 0.9em;
  cursor: pointer;
  margin-top: auto;
}
.fieldbar-drawer {
  position: fixed;
  top: 0;
  right: -100%;
  width: 350px;
  height: 100vh;
  max-height: 100vh;
  background: white;
  box-shadow: -2px 0 6px rgba(0, 0, 0, 0.1);
  transition: right 0.4s ease;
  overflow-y: auto;
  z-index: 1000;
  display: flex;
  flex-direction: column;
}
.fieldbar-drawer.open {
  right: 0;
}
.fieldbar-drawer-close {
  position: absolute;
  top: 0.5em;
  right: 1em;
  font-size: 1.5em;
  cursor: pointer;
  z-index: 10;
}
.fieldbar-drawer h3 {
  font-family: 'Gotham Medium', sans-serif;
  font-size: 1.2em;
  text-transform: uppercase;
  position: sticky;
  top: 0;
  background: white;
  z-index: 5;
  padding: 1em 1.5em;
  border-bottom: 1px solid #eee;
}
#fbRegionList {
  padding-left: 2em;
  padding-right: 1.5em;
  padding-bottom: 2em;
}
#fbRegionList strong {
  font-family: 'Gotham Narrow Book', sans-serif;
  text-transform: uppercase;
  font-size: 0.85em;
  display: block;
  margin: 1em 0 0.5em;
}

.fb-no-results {
  display: none;
  text-align: center;
  color: #486A7A;
  font-size: 0.9em;
  margin-top: 1em;
  line-height: 1.6em;
}

.fb-underline-link {
  position: relative;
  text-decoration: none;
  color: #486A7A;
  transition: color 0.4s ease;
  padding-bottom: 2px;
}

.fb-underline-link::after {
  content: "";
  position: absolute;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 1px;
  background-color: #486A7A;
  transform: scaleX(1);
  transform-origin: right;
  transition: transform 0.5s ease, background-color 0.5s ease;
}

.fb-underline-link:hover {
  color: #FF8A3D;
}

.fb-underline-link:hover::after {
  transform: scaleX(0);
  background-color: #FF8A3D;
}

@media screen and (max-width: 768px) {
  .fieldbar-retailers-map {
    flex-direction: column;
    height: auto;
   min-height: 135vh;
  }

  .fieldbar-retailers-map__map {
    order: -1; /* Ensure map comes first */
    width: 100%;
    height: 60vh; /* Updated from 80vh to 60vh */
  }

  .fieldbar-retailers-map__panel {
    order: 2;
    width: 100%;
    padding: 1.5em 1em;
    background: #F3EFEB;
  }

  .fieldbar-retailers-map__header {
    position: static;
    height: auto;
    padding-bottom: 1em;
  }

  .fieldbar-retailers-map__header h2 {
    font-size: 1.2em;
  }

  .fieldbar-retailers-map__list {
    max-height: none;
    overflow: visible;
  }

  .view-all-button {
    font-size: 0.95em;
    padding: 1em;
  }

  .leaflet-popup-content {
    font-size: 1em;
    padding: 1em 1.2em !important;
  }

  .leaflet-popup-content strong {
    font-size: 1.2em;
  }

  .leaflet-popup-content .popup-button {
    font-size: 0.75em;
    width: 100%;
  }

  .fieldbar-drawer {
    width: 100%;
  }
}




</style>

<div class="fieldbar-retailers-map">
