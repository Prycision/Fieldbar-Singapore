document.addEventListener("DOMContentLoaded", function () {
  const map = L.map("fb-map", {
    minZoom: 2,
    maxBounds: [[-85, -180], [85, 180]],
    maxBoundsViscosity: 0.8
  });

  L.tileLayer("https://{s}.basemaps.cartocdn.com/light_all/{z}/{x}/{y}{r}.png", {
    attribution: "© OpenStreetMap, CartoDB"
  }).addTo(map);

  const bounds = L.latLngBounds();
  const markerMap = {};
  const retailerList = document.getElementById("fbRetailerList");
  const regionList = {};

  retailers.forEach((retailer, index) => {
    const card = document.createElement("div");
    card.className = "retailer-card";

    // Collect search terms: name + addresses
    let searchTerms = retailer.name.toLowerCase();
    retailer.locations.forEach(loc => {
      if (loc.address) {
        searchTerms += ' ' + loc.address.toLowerCase();
      }
    });
    card.dataset.search = searchTerms;

    card.id = `retailer-${index}`;

    const heading = document.createElement("h3");
    heading.innerHTML = `${retailer.flagship ? `<img src="https://cdn.shopify.com/s/files/1/0729/3753/7840/files/favicon.png?v=1678097297" alt="Flagship" class="flagship-icon" /> ` : ''}${retailer.name}`;
    card.appendChild(heading);

    heading.addEventListener("click", () => {
      card.classList.toggle("active");

      const marker = markerMap[index];
      if (marker) {
        map.flyTo(marker.getLatLng(), 12, {
          animate: true,
          duration: 2.2,
          easeLinearity: 0.15 
        });
      }

      const mapEl = document.getElementById("fb-map");
      if (window.innerWidth < 768) {
        mapEl.scrollIntoView({ behavior: 'smooth' });
      }

      document.querySelectorAll('.retailer-card details').forEach((el, i) => {
        if (i !== index) el.open = false;
      });

      const details = card.querySelector('details');
      if (details) details.open = true;
    });

    retailer.locations.forEach(loc => {
      const details = document.createElement("details");
      const summary = document.createElement("summary");
      summary.textContent = retailer.online_only
        ? "Available online only"
        : loc.address;

      const content = document.createElement("div");
      if (loc.opening) {
        const open = document.createElement("p");
        open.textContent = loc.opening;
        content.appendChild(open);
      }
      if (retailer.online_only) {
        const note = document.createElement("span");
        note.className = "online-only-note";
        note.textContent = "Available online only";
        content.appendChild(note);
      }

      details.appendChild(summary);
      details.appendChild(content);
      card.appendChild(details);

      const iconUrl = 'https://cdn.shopify.com/s/files/1/0729/3753/7840/files/f-icon-map.svg?v=1750163830';
      const marker = L.marker([loc.lat, loc.lng], {
        icon: L.divIcon({
          html: `<div style="width: 32px; height: 48px; clip-path: path('M16 0 C24 0 32 8 32 16 C32 24 16 48 16 48 C16 48 0 24 0 16 C0 8 8 0 16 0 Z'); background-color: #002B4D; display: flex; align-items: center; justify-content: center;">
            <img src="${iconUrl}" width="24" height="24" style="transform: translateY(-6px);" />
          </div>`,
          iconSize: [32, 48],
          iconAnchor: [16, 48],
          className: ''
        })
      }).addTo(map);

      markerMap[index] = marker;

      marker.bindPopup(`
        <strong>${retailer.name}</strong><br/>
        ${loc.address}<br/>
       ${retailer.online_only
  ? `<div class="online-only-note">Available online only</div>`
  : `<a href="https://www.google.com/maps/search/?api=1&query=${encodeURIComponent(loc.address)}" target="_blank" class="popup-button">Get Directions</a>`}
      `);

      bounds.extend([loc.lat, loc.lng]);

      marker.on('click', () => {
        map.flyTo([loc.lat, loc.lng], 12, {
          animate: true,
          duration: 1.2,
          easeLinearity: 0.25
        });

        const cardEl = document.getElementById(`retailer-${index}`);
        if (cardEl) {
          cardEl.scrollIntoView({ behavior: 'smooth', block: 'start' });
          cardEl.classList.add('active');

          const details = cardEl.querySelector('details');
          if (details) details.open = true;

          document.querySelectorAll('.retailer-card details').forEach((el, i) => {
            if (i !== index) el.open = false;
          });

          setTimeout(() => cardEl.classList.remove('active'), 2000);
        }
      });
    });

    retailerList.appendChild(card);

    if (!regionList[retailer.region]) regionList[retailer.region] = [];
    regionList[retailer.region].push(retailer);
  });

  map.fitBounds(bounds.pad(0.3));

  document.getElementById("fbSearch").addEventListener("input", e => {
    const term = e.target.value.toLowerCase();
    let anyMatch = false;

    document.querySelectorAll(".retailer-card").forEach(card => {
      const match = card.dataset.search.includes(term);
      card.style.display = match ? "block" : "none";
      if (match) anyMatch = true;
    });

    document.getElementById("fbNoResults").style.display = anyMatch ? "none" : "block";
  });

  const regionContainer = document.getElementById("fbRegionList");
  for (const region in regionList) {
    const group = document.createElement("div");
    group.innerHTML = `<strong>${region}</strong><ul style="margin:0 0 1em;padding-left:1em">` +
      regionList[region].map(r => `<li>${r.name}</li>`).join("") +
      `</ul>`;
    regionContainer.appendChild(group);
  }
});
</script>


